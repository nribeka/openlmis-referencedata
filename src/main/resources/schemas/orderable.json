{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Orderable",
  "description": "A single orderable",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "productCode": {
      "type": "string",
      "title": "productCode"
    },
    "fullProductName": {
      "type": "string",
      "title": "fullProductName"
    },
    "description": {
      "type": "string",
      "title": "description"
    },
    "netContent": {
      "type": "number",
      "title": "netContent"
    },
    "packRoundingThreshold": {
      "type": "integer",
      "title": "packRoundingThreshold"
    },
    "roundToZero": {
      "type": "boolean",
      "title": "roundToZero"
    },
    "programs": {
      "type": "array",
      "title": "programs",
      "items": {
        "type": "object",
        "$ref": "programOrderableDto.json"
      },
      "uniqueItems": true
    },
    "identifiers": {
      "type": "object",
      "title": "identifiers"
    },
    "extraData": {
      "type": "object",
      "title": "extraData"
    },
    "versionId": {
      "type": "integer",
      "title": "versionId",
      "description": "The version number of an orderable. This property is required to update the orderable. Leave it blank when creating a new resource."
    }
  },
  "required": ["productCode", "packRoundingThreshold", "roundToZero", "netContent"]
}
